{
  "name": "Lightdash",
  "dockerComposeFile": "docker-compose.gautam.yml",
  "service": "lightdash-dev",
  "workspaceFolder": "/workspaces/lightdash",
  "shutdownAction": "stopCompose",
  "postCreateCommand": "git submodule update --init && ./scripts/build.sh && ./scripts/seed-jaffle.sh && ./scripts/migrate.sh && ./scripts/seed-lightdash.sh",
  "forwardPorts": [3000, 8080],
  "remoteEnv": {
    "LIGHTDASH_CONFIG_FILE": "/workspaces/lightdash/lightdash.yml",
    "DBT_DEMO_DIR": "/workspaces/lightdash/examples/full-jaffle-shop-demo",
    "PORT":"8080",
    "SITE_URL":"http://localhost:3000",
    "DOCKERFILE_TARGET":"dev",
    "PGHOST":"db-dev",
    "PGPORT":"5432",
    "PGUSER":"postgres",
    "PGPASSWORD":"password",
    "PGDATABASE":"postgres",
    "SECURE_COOKIES":"false",
    "TRUST_PROXY":"false",
    "LIGHTDASH_SECRET":"'not very secret'",
    "RUDDERSTACK_WRITE_KEY":"1vikeGadtB0Y0oRDFNL2Prdhkbp",
    "RUDDERSTACK_DATA_PLANE_URL":"https://analytics.lightdash.com",
    "SENTRY_DSN":"''",
    "NODE_ENV":"development",
    "LIGHTDASH_LOG_LEVEL":"debug",
    "LIGHTDASH_QUERY_MAX_LIMIT":"5000",
    "AUTH_GOOGLE_OAUTH2_CLIENT_ID":"",
    "AUTH_GOOGLE_OAUTH2_CLIENT_SECRET":"",
    "AUTH_DISABLE_PASSWORD_AUTHENTICATION":"false",
    "ALLOW_MULTIPLE_ORGS":"false",
    "DBT_PROJECT_DIR":".",
  },
  "extensions": ["mtxr.sqltools", "mtxr.sqltools-driver-pg"],
  "settings": {
    "git.autofetch": true,
    "sqltools.autoConnectTo": "Lightdash db",
    "sqltools.connections": [
      {
        "name": "Lightdash db",
        "driver": "PostgreSQL",
        "previewLimit": 50,
        "server": "db-dev",
        "port": 5432,
        "database": "postgres",
        "username": "postgres",
        "password": "password"
      }
    ]
  }
}
